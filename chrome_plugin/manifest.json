{
   "background": {
      "page": "background/background.html",
      "persistent": false
   },
   "browser_action": {
      "default_icon": "images/logo_296.png",
      "default_popup": "functionnalities/popup.html"
   },
   "chrome_url_overrides": {
      "newtab": "functionnalities/homepage.html"
   },
   "commands": {
      "logout-ease": {
         "description": "Logout from ease and all connected websites.",
         "suggested_key": {
            "default": "Ctrl+L",
            "mac": "Ctrl+L"
         }
      },
      "open-ease": {
         "description": "Open Ease in a new tab.",
         "suggested_key": {
            "default": "Ctrl+Space",
            "mac": "Ctrl+Space"
         }
      }
   },
   "content_scripts": [ {
      "js": [ "jquery-3.1.0.js", "tools/extension.js", "easeActions/scrapping.js", "easeActions/easeListener.js", "easeActions/updatesSender.js" ],
      "matches": [ "https://ease.space/*", "https://www.ease.space/*", "http://ease.space/*", "http://www.ease.space/*", "https://localhost:8443/*", "https://turfu.ease.space:8443/*"],
      "run_at": "document_end"
   }, {
      "all_frames": true,
      "js": [ "jquery-3.1.0.js", "tools/extensionLight.js", "checkConnections/connectionListener.js", "tools/extension.js", "contentScripts/actions.js", "contentScripts/connect.js" ],
      "matches": [ "\u003Call_urls>" ]
   }, {
      "js": [ "tools/extensionLight.js", "facebookCo/checkFacebookConnection.js" ],
      "matches": [ "https://www.facebook.com/*" ],
      "run_at": "document_end"
   }, {
      "css": [ "overlay/overlayScrap.css" ],
      "js": [ "jquery-3.1.0.js", "tools/extension.js", "overlay/injectOverlayScrap.js", "scrappingTuto/scrapFacebook.js" ],
      "matches": [ "https://www.facebook.com/*" ]
   }, {
      "css": [ "overlay/overlayScrap.css" ],
      "js": [ "jquery-3.1.0.js", "tools/extension.js", "overlay/injectOverlayScrap.js", "scrappingTuto/scrapChrome.js" ],
      "matches": [ "https://passwords.google.com/*", "https://accounts.google.com/*", "https://myaccount.google.com/*" ]
   }, {
      "css": [ "overlay/overlayScrap.css" ],
      "js": [ "jquery-3.1.0.js", "tools/extension.js", "overlay/injectOverlayScrap.js", "scrappingTuto/scrapLinkedin.js" ],
      "matches": [ "https://www.linkedin.com/*" ]
   } ],
   "description": "Ease extension, log you in and out",
   "icons": {
      "128": "images/logo_128.png"
   },
   "key":"",
   "manifest_version": 2,
   "name": "Ease",
   "permissions": [ "tabs", "\u003Call_urls>", "http://*/*", "https://*/*", "webNavigation", "storage", "identity", "identity.email", "contentSettings", "privacy" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.5.9",
   "web_accessible_resources": [ "scrappingTuto/logo.png", "scrappingTuto/facebook.png", "scrappingTuto/linkedin.png", "scrappingTuto/chrome.png" ]
}
